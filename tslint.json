{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "ban-types": false,
        "interface-name": false,
        "max-classes-per-file": false,
        "no-any": true,
        "object-literal-sort-keys": false,
        "quotemark": [
            true,
            "single"
        ],
        "triple-equals": [
            true,
            "allow-null-check",
            "allow-undefined-check"
        ]
    },
    "rulesDirectory": []
}
